local Setting = win:Tab("Setting","http://www.roblox.com/asset/?id=7040347038")

Setting:Button("Destroy UI", "", function()
	if game:GetService("CoreGui"):FindFirstChild("MOKURO Gui") then
		game:GetService("CoreGui"):FindFirstChild("MOKURO Gui"):Destroy()
	end
end)

Setting:Button("Copy Link Discord : MOKURO HUB", "", function()
	setclipboard("https://discord.gg/tgDeYtEm")
end)

Setting:Line()

if _G.FastAttack == true then
	FastAct = true
elseif _G.FastAttack == false then
	FastAct = false
else
	FastAct = true
end

Setting:Toggle("Fast Attack", "", FastAct, function(vu)
    FastAttack = vu
end)

spawn(function()
    pcall(function()
        while wait() do
            if Auto_Farm or _G.AutoDonSwan_Hop or _G.Auto_Dark_Dagger_Hop or _G.Auto_indra_Hop or _G.Pole_Hop or AutoCitizen or KillPlayer or Elite or AutoEcto or AutoBartilo or AutoThird or Auto_Newworld or AutoFarmBoss or FarmAllBoss then
                FastActive = true
            else
                FastActive = false
            end
        end
    end)
end)

spawn(function()
	while wait(.1) do
		if Factory and Core then
			FastActive = true
		else
			FastActive = false
		end
	end
end)

spawn(function()
	game:GetService("RunService").Heartbeat:Connect(function()
		if FastAttack and FastActive then
			pcall(function()
				game:GetService'VirtualUser':CaptureController()
				game:GetService('VirtualUser'):ClickButton1(Vector2.new(0.959,0.959))
			end)
		end
	end)
end)

spawn(function()
	while game:GetService("RunService").Heartbeat:wait() do
		if FastAttack and FastActive then
			pcall(function()
				require(game:GetService("Players").LocalPlayer.PlayerScripts.CombatFramework).activeController.timeToNextAttack = 0
			end)
		end
	end
end)

