local win = Flux:Window("BANER HUB", "[🌊 SEA 3] Blox Fruits", _G.Color1, _G.Color2, _G.ToggleKey)

local AutoFarm = win:Tab("Main", "http://www.roblox.com/asset/?id=7040391851")

Time = AutoFarm:Label("Server Time")

function UpdateTime()
    local GameTime = math.floor(workspace.DistributedGameTime+0.5)
    local Hour = math.floor(GameTime/(60^2))%24
    local Minute = math.floor(GameTime/(60^1))%60
    local Second = math.floor(GameTime/(60^0))%60
    Time:Refresh("Hour : "..Hour.." Minute : "..Minute.." Second : "..Second)
end
spawn(function()
    while true do
        UpdateTime()
        game:GetService("RunService").Heartbeat:Wait()
    end
end)

Client = AutoFarm:Label("Client")

function UpdateClient()
    local Ping = game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString()
    local Fps = (workspace:GetRealPhysicsFPS())
    Client:Refresh("Fps : "..Fps.." Ping : "..Ping)
end

spawn(function()
    while true do wait(.1)
        UpdateClient()
    end
end)

