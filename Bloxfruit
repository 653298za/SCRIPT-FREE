local Players = win:Tab("Players","http://www.roblox.com/asset/?id=7252023075")

SelectKillWeapon = ""

Players:Line()

Player = ""
local SelectPlayer = Players:Dropdown("Selected Player", PlayerName, function(vu)
    SelectedKillPlayer = vu
end)

Players:Button("Refresh Player", "", function()
    PlayerName = {}
    SelectPlayer:Clear()
    for i,v in pairs(game.Players:GetChildren()) do
		if v.Name ~= game.Players.LocalPlayer.Name then
	        SelectPlayer:Add(v.Name)
		end
    end
end)

Players:Line()

Players:Button("Teleport To Player", "", function()
    local plr1 = game.Players.LocalPlayer.Character
    local plr2 = game.Players:FindFirstChild(SelectedKillPlayer) 
    plr1.HumanoidRootPart.CFrame = plr2.Character.HumanoidRootPart.CFrame
end)

Players:Toggle("Spectate Player", "", false, function(vu)
    Spectate = vu
end)

Players:Toggle("Safe Mode", "", false, function(vu)
    SafeMode = vu
end)

Players:Line()

local SelectWeapon = Players:Dropdown("Select Weapon",Wapon,function(Value)
    SelectKillWeapon = Value
end)

Players:Button("Refresh Weapon","",function()
    SelectWeapon:Clear()
	for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
		if v:IsA("Tool") then
			SelectWeapon:Add(v.Name)
		end
	end
	for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
		if v:IsA("Tool") then
			SelectWeapon:Add(v.Name)
		end
	end
end)

Players:Toggle("Kill Player Over", "", false, function(vu)
    KillPlayer = vu
end)

Players:Toggle("Kill Player(50%)","",false,function(bool)
                                    KillPlayer = bool
                                    if KillPlayer == false then
                                        game.Players:FindFirstChild(SelectedKillPlayer).Character.HumanoidRootPart.Size = Vector3.new(2, 2, 1)
                                    end
                                    while KillPlayer do wait()
                                        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players:FindFirstChild(SelectedKillPlayer).Character.HumanoidRootPart.CFrame * CFrame.new(25, 0, 7)
                                        game.Players:FindFirstChild(SelectedKillPlayer).Character.HumanoidRootPart.Size = Vector3.new(60,60,60)
                                        game:GetService'VirtualUser':CaptureController()
                                        game:GetService'VirtualUser':Button1Down(Vector2.new(1280, 672))
                                    end
                                end)

Players:Toggle("Aimbot Gun", "", false, function(vu)
	Aimbot = vu
end)

Players:Toggle("Auto Aimbot Skill", "", false, function(vu)
	AimbotSkill = vu
end)

Players:Toggle("Nearest Aimbot Skill", "", false, function(vu)
	AimSkillNearest = vu
end)

Players:Line()

spawn(function()
	pcall(function()
		while wait() do
			for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do  
				if v:IsA("Tool") then
					if v:FindFirstChild("RemoteFunctionShoot") then 
						SelectToolWeaponGun = v.Name
					end
				end
			end
		end
	end)
end)

spawn(function()
	pcall(function()
		while wait() do
			for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do  
				if v:IsA("Tool") then
					if v.ToolTip == "Melee" then 
						SelectToolWeaponMelee = v.Name
					end
				end
			end
		end
	end)
end)

Players:Slider("WalkSpeed", "", 0,1000,16, function(t)
    game:GetService("Players").LocalPlayer.Character.Humanoid.WalkSpeed = t
    while t do wait(1)
        game:GetService("Players").LocalPlayer.Character.Movement.Disabled = true
    end
end)

Players:Slider("Jump Power", "", 0,1000,50, function(t)
    game:GetService("Players").LocalPlayer.Character.Humanoid.JumpPower = t
end)

Players:Slider("Gravity","",0,500,196, function(t)
    workspace.Gravity = t
end)

