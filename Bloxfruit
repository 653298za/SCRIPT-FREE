function FarmBossssss()
	if AutoFarmBoss then
		CheckBossQuest()
		if SelectBoss == "Longma [Lv. 2000] [Boss]" or SelectBoss == "Don Swan [Lv. 1000] [Boss]" or SelectBoss == "Cursed Captain [Lv. 1325] [Raid Boss]" then
			if game:GetService("Workspace").Enemies:FindFirstChild(SelectBoss) then
				for i,v in pairs(game.Workspace.Enemies:GetDescendants()) do
					if AutoFarmBoss and v:IsA("Model") and v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 and v.Name == BossMon then
						repeat game:GetService("RunService").Heartbeat:wait()
							EquipWeapon(SelectWeaponBoss)
							if game:GetService("Workspace").Enemies:FindFirstChild(SelectBoss) then
								game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 10, 8)
								require(game:GetService("Players").LocalPlayer.PlayerScripts.CombatFramework).activeController.hitboxMagnitude = 1000
								game:GetService("VirtualUser"):CaptureController()
								game:GetService("VirtualUser"):Button1Down(Vector2.new(1280, 672))
								AutoFarmSkill = true
								sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
							else
								AutoFarmSkill = false
								game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameBoss
							end
						until AutoFarmBoss == false or not v.Parent or v.Humanoid.Health <= 0
						if not AutoFarmBoss then
							AutoFarmSkill = false
							game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameBoss
						end
					else
						AutoFarmSkill = false
						game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameBoss
					end
				end
			else
				AutoFarmSkill = false
				game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameBoss
			end
		elseif SelectBoss == "Order [Lv. 1250] [Raid Boss]" then
			for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
				if game:GetService("Workspace").Enemies:FindFirstChild("Order [Lv. 1250] [Raid Boss]") then
					if v.Name == "Order [Lv. 1250] [Raid Boss]" then
						repeat game:GetService("RunService").Heartbeat:wait()
							EquipWeapon(SelectWeaponBoss)
							game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 10, 8)
							require(game:GetService("Players").LocalPlayer.PlayerScripts.CombatFramework).activeController.hitboxMagnitude = 2000
							game:GetService("VirtualUser"):CaptureController()
							game:GetService("VirtualUser"):Button1Down(Vector2.new(1280, 672))
							sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
						until AutoFarmBoss == false or not v.Parent or v.Humanoid.Health <= 0
						if not AutoFarmBoss then
							AutoFarmSkill = false
							game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-6217.2021484375, 28.047645568848, -5053.1357421875)
						end
					end
				else
					AutoFarmSkill = false
					game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-6217.2021484375, 28.047645568848, -5053.1357421875)
				end
			end
		elseif SelectBoss == "rip_indra [Lv. 5000] [Raid Boss]" or SelectBoss == "rip_indra True Form [Lv. 5000] [Raid Boss]" then
			for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
				if game:GetService("Workspace").Enemies:FindFirstChild("rip_indra [Lv. 5000] [Raid Boss]") or game:GetService("Workspace").Enemies:FindFirstChild("rip_indra True Form [Lv. 5000] [Raid Boss]") then
					if v.Name == "rip_indra [Lv. 5000] [Raid Boss]" or v.Name == "rip_indra True Form [Lv. 5000] [Raid Boss]" then
						repeat game:GetService("RunService").Heartbeat:wait()
							EquipWeapon(SelectWeaponBoss)
							game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 10, 8)
							AutoFarmSkill = true
							require(game:GetService("Players").LocalPlayer.PlayerScripts.CombatFramework).activeController.hitboxMagnitude = 1000
							game:GetService("VirtualUser"):CaptureController()
							game:GetService("VirtualUser"):Button1Down(Vector2.new(1280, 672))
							sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
						until AutoFarmBoss == false or not v.Parent or v.Humanoid.Health <= 0
						if not AutoFarmBoss then
							AutoFarmSkill = false
							game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-5415.3920898438, 505.74133300781, -2814.0166015625)
						end
					end
				else
					AutoFarmSkill = false
					game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-5415.3920898438, 505.74133300781, -2814.0166015625)
				end
			end
		else
			if game:GetService("Workspace").Enemies:FindFirstChild(SelectBoss) or game:GetService("ReplicatedStorage"):FindFirstChild(SelectBoss) then
				if BossQuest then
					if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == false then
						AutoFarmSkill = false
						game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameQ
						wait(1.1)
						game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", NameQuestBoss, QuestLvBoss)
						game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameBoss
					elseif game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == true then
						AutoFarmSkill = true
						if game:GetService("Workspace").Enemies:FindFirstChild(SelectBoss) then
							for i,v in pairs(game:GetService("Workspace").Enemies:GetDescendants()) do
								if AutoFarmBoss and v:IsA("Model") and v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 and v.Name == BossMon then
									repeat game:GetService("RunService").Heartbeat:wait()
										EquipWeapon(SelectWeaponBoss)
										if string.find(game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Container.QuestReward.Title.Text, RewardBoss) then
											if game:GetService("Workspace").Enemies:FindFirstChild(SelectBoss) then
												game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 10, 8)
												AutoFarmSkill = true
												require(game:GetService("Players").LocalPlayer.PlayerScripts.CombatFramework).activeController.hitboxMagnitude = 1000
												game:GetService("VirtualUser"):CaptureController()
												game:GetService("VirtualUser"):Button1Down(Vector2.new(1280, 672))
												sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
											else
												AutoFarmSkill = false
												game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameBoss
											end
										else
											AutoFarmSkill = false
											game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AbandonQuest")
										end
									until AutoFarmBoss == false or not v.Parent or v.Humanoid.Health <= 0 or game.Players.LocalPlayer.PlayerGui.Main.Quest.Visible == false
									if not AutoFarmBoss then
										game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameBoss
									end
								end
							end
						else
							AutoFarmSkill = false
							game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameBoss
						end
					end
				else
					if game:GetService("Workspace").Enemies:FindFirstChild(SelectBoss) then
						for i,v in pairs(game:GetService("Workspace").Enemies:GetDescendants()) do
							if AutoFarmBoss and v:IsA("Model") and v:FindFirstChild("Humanoid") and v:FindFirstChild("HumanoidRootPart") and v.Humanoid.Health > 0 and v.Name == BossMon then
								repeat game:GetService("RunService").Heartbeat:wait()
									EquipWeapon(SelectWeaponBoss)
									if game:GetService("Workspace").Enemies:FindFirstChild(SelectBoss) then
										game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0, 10, 8)
										AutoFarmSkill = true
										require(game:GetService("Players").LocalPlayer.PlayerScripts.CombatFramework).activeController.hitboxMagnitude = 1000
										game:GetService("VirtualUser"):CaptureController()
										game:GetService("VirtualUser"):Button1Down(Vector2.new(1280, 672))
										sethiddenproperty(game.Players.LocalPlayer, "SimulationRadius", math.huge)
									else
										AutoFarmSkill = false
										game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameBoss
									end
								until AutoFarmBoss == false or not v.Parent or v.Humanoid.Health <= 0
								if not AutoFarmBoss then
									AutoFarmSkill = false
									game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameBoss
								end
							end
						end
					else
						AutoFarmSkill = false
						game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameBoss
					end
				end
			else
				game:GetService("Players").LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameBoss
			end
		end
	end
end

SelectBoss = ""

function AutoFarmAllBoss()
	if SelectBoss == "" then
		if SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("The Gorilla King [Lv. 25] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("The Gorilla King [Lv. 25] [Boss]")) and game.Players.LocalPlayer.Data.Level.Value >= 20 then
			SelectBoss = "The Gorilla King [Lv. 25] [Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Bobby [Lv. 55] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Bobby [Lv. 55] [Boss]")) and game.Players.LocalPlayer.Data.Level.Value >= 50 then
			SelectBoss = "Bobby [Lv. 55] [Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Yeti [Lv. 110] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Yeti [Lv. 110] [Boss]")) and game.Players.LocalPlayer.Data.Level.Value >= 105 then
			SelectBoss = "Yeti [Lv. 110] [Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Mob Leader [Lv. 120] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Mob Leader [Lv. 120] [Boss]")) then
			SelectBoss = "Mob Leader [Lv. 120] [Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Vice Admiral [Lv. 130] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Vice Admiral [Lv. 130] [Boss]")) and game.Players.LocalPlayer.Data.Level.Value >= 130 then
			SelectBoss = "Vice Admiral [Lv. 130] [Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Warden [Lv. 175] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Warden [Lv. 175] [Boss]")) and game.Players.LocalPlayer.Data.Level.Value >= 175 then
			SelectBoss = "Warden [Lv. 175] [Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Saber Expert [Lv. 200] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Saber Expert [Lv. 200] [Boss]")) and game.Workspace.Map.Jungle.Final.Part.Transparency == 1 then
			SelectBoss = "Saber Expert [Lv. 200] [Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Swan [Lv. 225] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Swan [Lv. 225] [Boss]")) and game.Players.LocalPlayer.Data.Level.Value >= 250 then
			SelectBoss = "Swan [Lv. 225] [Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Magma Admiral [Lv. 350] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Magma Admiral [Lv. 350] [Boss]")) and game.Players.LocalPlayer.Data.Level.Value >= 350 then
			SelectBoss = "Magma Admiral [Lv. 350] [Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Fishman Lord [Lv. 425] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Fishman Lord [Lv. 425] [Boss]")) and game.Players.LocalPlayer.Data.Level.Value >= 425 then
			SelectBoss = "Fishman Lord [Lv. 425] [Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Wysper [Lv. 500] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Wysper [Lv. 500] [Boss]")) and game.Players.LocalPlayer.Data.Level.Value >= 500 then
			SelectBoss = "Wysper [Lv. 500] [Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Thunder God [Lv. 575] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Thunder God [Lv. 575] [Boss]")) and game.Players.LocalPlayer.Data.Level.Value >= 575 then
			SelectBoss = "Thunder God [Lv. 575] [Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Cyborg [Lv. 675] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Cyborg [Lv. 675] [Boss]")) and game.Players.LocalPlayer.Data.Level.Value >= 675 then
			SelectBoss = "Cyborg [Lv. 675] [Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Greybeard [Lv. 750] [Raid Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Greybeard [Lv. 750] [Raid Boss]")) then
			SelectBoss = "Greybeard [Lv. 750] [Raid Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Diamond [Lv. 750] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Diamond [Lv. 750] [Boss]")) and game.Players.LocalPlayer.Data.Level.Value >= 750 then
			SelectBoss = "Diamond [Lv. 750] [Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Jeremy [Lv. 850] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Jeremy [Lv. 850] [Boss]")) and game.Players.LocalPlayer.Data.Level.Value >= 850 then
			SelectBoss = "Jeremy [Lv. 850] [Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Fajita [Lv. 925] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Fajita [Lv. 925] [Boss]")) and game.Players.LocalPlayer.Data.Level.Value >= 925 then
			SelectBoss = "Fajita [Lv. 925] [Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Don Swan [Lv. 1000] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Don Swan [Lv. 1000] [Boss]")) and game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("TalkTrevor","1") == 0 then
			SelectBoss = "Don Swan [Lv. 1000] [Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Smoke Admiral [Lv. 1150] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Smoke Admiral [Lv. 1150] [Boss]")) and game.Players.LocalPlayer.Data.Level.Value >= 1150 then
			SelectBoss = "Smoke Admiral [Lv. 1150] [Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Cursed Captain [Lv. 1325] [Raid Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Cursed Captain [Lv. 1325] [Raid Boss]")) and game.Players.LocalPlayer.Data.Level.Value >= 1325 then
			SelectBoss = "Cursed Captain [Lv. 1325] [Raid Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Awakened Ice Admiral [Lv. 1400] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Awakened Ice Admiral [Lv. 1400] [Boss]")) and game.Players.LocalPlayer.Data.Level.Value >= 1400 then
			SelectBoss = "Awakened Ice Admiral [Lv. 1400] [Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Tide Keeper [Lv. 1475] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Tide Keeper [Lv. 1475] [Boss]")) and game.Players.LocalPlayer.Data.Level.Value >= 1475 then
			SelectBoss = "Tide Keeper [Lv. 1475] [Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Darkbeard [Lv. 1000] [Raid Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Darkbeard [Lv. 1000] [Raid Boss]")) then
			SelectBoss = "Darkbeard [Lv. 1000] [Raid Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Stone [Lv. 1550] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Stone [Lv. 1550] [Boss]")) and game.Players.LocalPlayer.Data.Level.Value >= 1550 then
			SelectBoss = "Stone [Lv. 1550] [Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Island Empress [Lv. 1675] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Island Empress [Lv. 1675] [Boss]")) and game.Players.LocalPlayer.Data.Level.Value >= 1675 then
			SelectBoss = "Island Empress [Lv. 1675] [Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Kilo Admiral [Lv. 1750] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Kilo Admiral [Lv. 1750] [Boss]")) and game.Players.LocalPlayer.Data.Level.Value >= 1750 then
			SelectBoss = "Kilo Admiral [Lv. 1750] [Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Captain Elephant [Lv. 1875] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Captain Elephant [Lv. 1875] [Boss]")) and game.Players.LocalPlayer.Data.Level.Value >= 1875 then
			SelectBoss = "Captain Elephant [Lv. 1875] [Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Beautiful Pirate [Lv. 1950] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Beautiful Pirate [Lv. 1950] [Boss]")) and game.Players.LocalPlayer.Data.Level.Value >= 1950 then
			SelectBoss = "Beautiful Pirate [Lv. 1950] [Boss]"
		elseif SelectBoss == "" and (game:GetService("Workspace").Enemies:FindFirstChild("Longma [Lv. 2000] [Boss]") or game:GetService("ReplicatedStorage"):FindFirstChild("Longma [Lv. 2000] [Boss]")) and not game:GetService("Workspace").Map.Turtle:FindFirstChild("TushitaGate") then
			SelectBoss = "Longma [Lv. 2000] [Boss]"
		end
	else
		if FarmAllBoss then
			if SelectBoss == "Mob Leader [Lv. 120] [Boss]" or SelectBoss == "Saber Expert [Lv. 200] [Boss]" or SelectBoss == "Don Swan [Lv. 1000] [Boss]" or SelectBoss == "Cursed Captain [Lv. 1325] [Raid Boss]" or SelectBoss == "Longma [Lv. 2000] [Boss]" or SelectBoss == "Darkbeard [Lv. 1000] [Raid Boss]" or SelectBoss == "Greybeard [Lv. 750] [Raid Boss]" then
				if game:GetService("Workspace").Enemies:FindFirstChild(SelectBoss) then
					CheckBossQuest()
					for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
						if v.Name == BossMon then
							if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
								repeat game:GetService("RunService").Heartbeat:wait()
									if game:GetService("Workspace").Enemies:FindFirstChild(BossMon) then
										EquipWeapon(SelectToolWeapon)
										v.HumanoidRootPart.CanCollide = false
										v.HumanoidRootPart.Size = Vector3.new(40,40,40)
										v.Humanoid:ChangeState(11)
										v.Humanoid:ChangeState(0)
										v.Humanoid:ChangeState(7)
										game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,15,5)
										game:GetService("VirtualUser"):CaptureController()
										game:GetService("VirtualUser"):Button1Down(Vector2.new(1280, 672))
									else
										game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameBoss
									end
								until v.Humanoid.Health <= 0 or not v.Parent or FarmAllBoss == false
								SelectBoss = ""
							end
						end
					end
				else
					CheckBossQuest()
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameBoss
				end
			else
				if game:GetService("Workspace").Enemies:FindFirstChild(SelectBoss) or game:GetService("ReplicatedStorage"):FindFirstChild(SelectBoss) then
					if game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == false then
						CheckBossQuest()
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameQ
						wait(1.2)
						game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("StartQuest", NameQuestBoss, QuestLvBoss)
					elseif game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == true then
						CheckBossQuest()
						if game:GetService("Workspace").Enemies:FindFirstChild(BossMon) then
							for i,v in pairs(game:GetService("Workspace").Enemies:GetChildren()) do
								if v.Name == BossMon then
									if v:FindFirstChild("HumanoidRootPart") and v:FindFirstChild("Humanoid") and v.Humanoid.Health > 0 then
										repeat game:GetService("RunService").Heartbeat:wait()
                                            if string.find(game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Container.QuestReward.Title.Text, RewardBoss) then
                                                if game:GetService("Workspace").Enemies:FindFirstChild(BossMon) then
													EquipWeapon(SelectToolWeapon)
                                                    v.HumanoidRootPart.CanCollide = false
                                                    v.HumanoidRootPart.Size = Vector3.new(40,40,40)
                                                    v.Humanoid:ChangeState(11)
                                                    v.Humanoid:ChangeState(0)
                                                    v.Humanoid:ChangeState(7)
                                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.HumanoidRootPart.CFrame * CFrame.new(0,15,5)
                                                    game:GetService("VirtualUser"):CaptureController()
                                                    game:GetService("VirtualUser"):Button1Down(Vector2.new(1280, 672))
                                                else
                                                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameBoss
                                                end
                                            else
                                                game:GetService("ReplicatedStorage").Remotes.CommF_:InvokeServer("AbandonQuest")
                                            end
										until v.Humanoid.Health <= 0 or not v.Parent or FarmAllBoss == false or game:GetService("Players").LocalPlayer.PlayerGui.Main.Quest.Visible == false
										SelectBoss = ""
									end
								end
							end
						else
							CheckBossQuest()
							game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrameBoss
                            SelectBoss = ""
						end
					end
				end
			end
            SelectBoss = ""
		end
	end
end

spawn(function()
	while wait(.1) do
		if _G.Auto_Farm_All_Boss_Hop then
			local Replicated = game:GetService("ReplicatedStorage")
			local Enemies = game:GetService("Workspace").Enemies
			if (not Replicated:FindFirstChild("The Gorilla King [Lv. 25] [Boss]") and not Replicated:FindFirstChild("Bobby [Lv. 55] [Boss]") and not Replicated:FindFirstChild("Yeti [Lv. 110] [Boss]") and not Replicated:FindFirstChild("Mob Leader [Lv. 120] [Boss]") and not Replicated:FindFirstChild("Vice Admiral [Lv. 130] [Boss]") and not Replicated:FindFirstChild("Warden [Lv. 175] [Boss]") and not Replicated:FindFirstChild("Chief Warden [Lv. 200] [Boss]") and not Replicated:FindFirstChild("Saber Expert [Lv. 200] [Boss]") and not Replicated:FindFirstChild("Swan [Lv. 225] [Boss]") and not Replicated:FindFirstChild("Magma Admiral [Lv. 350] [Boss]") and not Replicated:FindFirstChild("Fishman Lord [Lv. 425] [Boss]") and not Replicated:FindFirstChild("Wysper [Lv. 500] [Boss]") and not Replicated:FindFirstChild("Thunder God [Lv. 575] [Boss]") and not Replicated:FindFirstChild("Cyborg [Lv. 675] [Boss]") and not Replicated:FindFirstChild("Diamond [Lv. 750] [Boss]") and not Replicated:FindFirstChild("Jeremy [Lv. 850] [Boss]") and not Replicated:FindFirstChild("Fajita [Lv. 925] [Boss]") and not Replicated:FindFirstChild("Don Swan [Lv. 1000] [Boss]") and not Replicated:FindFirstChild("Smoke Admiral [Lv. 1150] [Boss]") and not Replicated:FindFirstChild("Cursed Captain [Lv. 1325] [Raid Boss]") and not Replicated:FindFirstChild("Awakened Ice Admiral [Lv. 1400] [Boss]") and not Replicated:FindFirstChild("Tide Keeper [Lv. 1475] [Boss]") and not Replicated:FindFirstChild("Stone [Lv. 1550] [Boss]") and not Replicated:FindFirstChild("Island Empress [Lv. 1675] [Boss]") and not Replicated:FindFirstChild("Kilo Admiral [Lv. 1750] [Boss]") and not Replicated:FindFirstChild("Captain Elephant [Lv. 1875] [Boss]") and not Replicated:FindFirstChild("Beautiful Pirate [Lv. 1950] [Boss]") and not Replicated:FindFirstChild("Longma [Lv. 2000] [Boss]")) and (not Replicated:FindFirstChild("The Gorilla King [Lv. 25] [Boss]") and not Enemies:FindFirstChild("Bobby [Lv. 55] [Boss]") and not Enemies:FindFirstChild("Yeti [Lv. 110] [Boss]") and not Enemies:FindFirstChild("Mob Leader [Lv. 120] [Boss]") and not Enemies:FindFirstChild("Vice Admiral [Lv. 130] [Boss]") and not Enemies:FindFirstChild("Warden [Lv. 175] [Boss]") and not Enemies:FindFirstChild("Chief Warden [Lv. 200] [Boss]") and not Enemies:FindFirstChild("Saber Expert [Lv. 200] [Boss]") and not Enemies:FindFirstChild("Swan [Lv. 225] [Boss]") and not Enemies:FindFirstChild("Magma Admiral [Lv. 350] [Boss]") and not Enemies:FindFirstChild("Fishman Lord [Lv. 425] [Boss]") and not Enemies:FindFirstChild("Wysper [Lv. 500] [Boss]") and not Enemies:FindFirstChild("Thunder God [Lv. 575] [Boss]") and not Enemies:FindFirstChild("Cyborg [Lv. 675] [Boss]") and not Enemies:FindFirstChild("Diamond [Lv. 750] [Boss]") and not Enemies:FindFirstChild("Jeremy [Lv. 850] [Boss]") and not Enemies:FindFirstChild("Fajita [Lv. 925] [Boss]") and not Enemies:FindFirstChild("Don Swan [Lv. 1000] [Boss]") and not Enemies:FindFirstChild("Smoke Admiral [Lv. 1150] [Boss]") and not Enemies:FindFirstChild("Cursed Captain [Lv. 1325] [Raid Boss]") and not Enemies:FindFirstChild("Awakened Ice Admiral [Lv. 1400] [Boss]") and not Enemies:FindFirstChild("Tide Keeper [Lv. 1475] [Boss]") and not Enemies:FindFirstChild("Stone [Lv. 1550] [Boss]") and not Enemies:FindFirstChild("Island Empress [Lv. 1675] [Boss]") and not Enemies:FindFirstChild("Kilo Admiral [Lv. 1750] [Boss]") and not Enemies:FindFirstChild("Captain Elephant [Lv. 1875] [Boss]") and not Enemies:FindFirstChild("Beautiful Pirate [Lv. 1950] [Boss]") and not Enemies:FindFirstChild("Longma [Lv. 2000] [Boss]")) then
				if game:GetService("Players").LocalPlayer.PlayerGui.Main.InCombat.Visible == false then
					game:GetService("Players")["LocalPlayer"].Character.HumanoidRootPart.CFrame = CFrame.new(-262.11901855469, 49325.69140625, -35272.49609375)
					wait(2)
					Teleport()
				elseif game:GetService("Players").LocalPlayer.PlayerGui.Main.InCombat.Visible == true then
					repeat game:GetService("RunService").Heartbeat:wait()
						local X = math.random(2500,5000)
						local Z = math.random(2500,5000)
						game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(X,20000,Y)
					until game:GetService("Players").LocalPlayer.PlayerGui.Main.InCombat.Visible == false
					Teleport()
				end
			end
		end
	end
end)

function KillPlayerfunc()
	if KillPlayer and Aimbot then
		EquipWeapon(SelectToolWeaponGun)
		AutoHaki()
		if HideHit then
			game.Players:FindFirstChild(SelectedKillPlayer).Character.HumanoidRootPart.Transparency = 1
		else
			game.Players:FindFirstChild(SelectedKillPlayer).Character.HumanoidRootPart.Transparency = .8
		end
		game.Players:FindFirstChild(SelectedKillPlayer).Character.HumanoidRootPart.CanCollide = false
		game.Players:FindFirstChild(SelectedKillPlayer).Character.HumanoidRootPart.Size = Vector3.new(60,60,60)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players:FindFirstChild(SelectedKillPlayer).Character.HumanoidRootPart.CFrame * CFrame.new(0,50,0)
		game:GetService("VirtualUser"):CaptureController()
		game:GetService("VirtualUser"):Button1Down(Vector2.new(1280, 672))
        if SkillZ then
			local args = {
				[1] = game.Players:FindFirstChild(SelectedKillPlayer).Character.HumanoidRootPart.Position
			}
			game:GetService("Players").LocalPlayer.Character[SelectToolWeaponGun].RemoteEvent:FireServer(unpack(args))
			local args = {
				[1] = "Z",
				[2] = Vector3.new(0,0,0)
			}
			game:GetService("Players").LocalPlayer.Character[SelectToolWeaponGun].RemoteFunction:InvokeServer(unpack(args))
		end
		if SkillX  then
			local args = {
				[1] = game.Players:FindFirstChild(SelectedKillPlayer).Character.HumanoidRootPart.Position
			}
			game:GetService("Players").LocalPlayer.Character[SelectToolWeaponGun].RemoteEvent:FireServer(unpack(args))
			local args = {
				[1] = "X",
				[2] = Vector3.new(0,0,0)
			}
			game:GetService("Players").LocalPlayer.Character[SelectToolWeaponGun].RemoteFunction:InvokeServer(unpack(args))
		end
	elseif KillPlayer then
		EquipWeapon(SelectKillWeapon)
		AutoHaki()
		if HideHit then
			game.Players:FindFirstChild(SelectedKillPlayer).Character.HumanoidRootPart.Transparency = 1
		else
			game.Players:FindFirstChild(SelectedKillPlayer).Character.HumanoidRootPart.Transparency = .8
		end
		game.Players:FindFirstChild(SelectedKillPlayer).Character.HumanoidRootPart.CanCollide = false
		game.Players:FindFirstChild(SelectedKillPlayer).Character.HumanoidRootPart.Size = Vector3.new(60,60,60)
		game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Players:FindFirstChild(SelectedKillPlayer).Character.HumanoidRootPart.CFrame * CFrame.new(0,13,7)
		game:GetService("VirtualUser"):CaptureController()
		game:GetService("VirtualUser"):Button1Down(Vector2.new(1280, 672))
		if SkillZ then
			local args = {
				[1] = game.Players:FindFirstChild(SelectedKillPlayer).Character.HumanoidRootPart.Position
			}
			game:GetService("Players").LocalPlayer.Character[game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").Name].RemoteEvent:FireServer(unpack(args))
			local args = {
				[1] = "Z",
				[2] = Vector3.new(0,0,0)
			}
			game:GetService("Players").LocalPlayer.Character[game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").Name].RemoteFunction:InvokeServer(unpack(args))
		end
		if SkillX  then
			local args = {
				[1] = game.Players:FindFirstChild(SelectedKillPlayer).Character.HumanoidRootPart.Position
			}
			game:GetService("Players").LocalPlayer.Character[game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").Name].RemoteEvent:FireServer(unpack(args))
			local args = {
				[1] = "X",
				[2] = Vector3.new(0,0,0)
			}
			game:GetService("Players").LocalPlayer.Character[game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").Name].RemoteFunction:InvokeServer(unpack(args))
		end
		if SkillV then
			local args = {
				[1] = game.Players:FindFirstChild(SelectedKillPlayer).Character.HumanoidRootPart.Position
			}
			game:GetService("Players").LocalPlayer.Character[game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").Name].RemoteEvent:FireServer(unpack(args))
			local args = {
				[1] = "X",
				[2] = Vector3.new(0,0,0)
			}
			game:GetService("Players").LocalPlayer.Character[game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").Name].RemoteFunction:InvokeServer(unpack(args))
		end
	end
end

